<p id="notice"><%= notice %></p>

<h1>Productos</h1>
 <div class="label"> <%= link_to 'Iniciar Sesion', new_user_session_path %></div>
  <div class="label"> <%= link_to 'registrarse', new_user_path  %></div>
<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Tipo</th>
      <th>Material</th>
      <th>Dimenciones</th>
      <th>Color</th>
      <th>Peso</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @productos.each do |producto| %>
      <tr>
        <td><%= producto.nombre %></td>
        <td><%= producto.descripcion %></td>
        <td><%= producto.tipo %></td>
        <td><%= producto.material %></td>
        <td><%= producto.dimenciones %></td>
        <td><%= producto.color %></td>
        <td><%= producto.peso %></td>
        <td><%= producto.precio %></td>
        <td><%= link_to 'Show', producto %></td>

      
      </tr>
    <% end %>
  </tbody>
</table>

<br>

